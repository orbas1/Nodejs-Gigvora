function parseWorkspaceId(rawValue) {
  if (!rawValue) {
    return undefined;
  }
  const numeric = Number.parseInt(rawValue, 10);
  return Number.isNaN(numeric) ? undefined : numeric;
}

function normalizePane(value) {
  if (!value) {
    return 'overview';
  }
  return VOL_PANES.has(value) ? value : 'overview';
}

