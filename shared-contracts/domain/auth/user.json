{
  "$ref": "#/definitions/Schema",
  "definitions": {
    "Schema": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "firstName": {
          "anyOf": [
            {
              "type": "string",
              "maxLength": 120
            },
            {
              "type": "null"
            }
          ]
        },
        "lastName": {
          "anyOf": [
            {
              "type": "string",
              "maxLength": 120
            },
            {
              "type": "null"
            }
          ]
        },
        "name": {
          "type": "string"
        },
        "address": {
          "anyOf": [
            {
              "type": "string",
              "maxLength": 255
            },
            {
              "type": "null"
            }
          ]
        },
        "location": {
          "anyOf": [
            {
              "type": "string",
              "maxLength": 255
            },
            {
              "type": "null"
            }
          ]
        },
        "geoLocation": {
          "anyOf": [
            {
              "type": "object",
              "properties": {
                "lat": {
                  "type": "number"
                },
                "lng": {
                  "type": "number"
                },
                "raw": {}
              },
              "additionalProperties": false
            },
            {
              "type": "null"
            }
          ]
        },
        "age": {
          "anyOf": [
            {
              "type": "integer",
              "minimum": 13
            },
            {
              "type": "null"
            }
          ]
        },
        "userType": {
          "type": "string",
          "enum": [
            "user",
            "company",
            "freelancer",
            "agency",
            "admin"
          ]
        },
        "twoFactorEnabled": {
          "type": "boolean"
        },
        "twoFactorMethod": {
          "type": "string",
          "enum": [
            "email",
            "app",
            "sms"
          ]
        },
        "lastLoginAt": {
          "anyOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "null"
            }
          ]
        },
        "googleId": {
          "type": [
            "string",
            "null"
          ]
        },
        "memberships": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "primaryDashboard": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "email",
        "firstName",
        "lastName",
        "name",
        "address",
        "location",
        "geoLocation",
        "age",
        "userType",
        "twoFactorEnabled",
        "twoFactorMethod",
        "lastLoginAt",
        "googleId",
        "memberships",
        "primaryDashboard"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
