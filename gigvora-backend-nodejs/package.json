{
  "name": "gigvora-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "search:sync": "node scripts/syncMeilisearch.js",
    "schemas:sync": "node scripts/syncDomainSchemas.js",
    "schemas:clients": "node scripts/generateDomainClients.js",
    "schemas:ci": "node scripts/generateDomainClients.js && node scripts/assertContractVersion.js",
    "schemas:version": "node scripts/bumpContractVersion.js",
    "config:validate": "node scripts/validateRuntimeConfig.js .env.example",
    "schema:export": "node scripts/generateSchemaArtifacts.js",
    "db:backup": "node scripts/databaseBackup.js backup",
    "db:verify": "node scripts/databaseBackup.js verify"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/instrumentation": "^0.51.1",
    "@opentelemetry/instrumentation-express": "^0.43.0",
    "@opentelemetry/instrumentation-http": "^0.51.1",
    "@opentelemetry/resources": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.9.0",
    "@opentelemetry/sdk-trace-node": "^1.9.0",
    "@opentelemetry/semantic-conventions": "^1.9.0",
    "@aws-sdk/client-s3": "^3.590.0",
    "@aws-sdk/s3-request-presigner": "^3.590.0",
    "agora-access-token": "^2.0.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "fs-extra": "^11.3.2",
    "ioredis": "^5.4.1",
    "google-auth-library": "^9.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "socket.io": "^4.7.5",
    "meilisearch": "^0.37.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.4",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13",
    "pino": "^9.3.0",
    "pino-http": "^9.0.0",
    "prom-client": "^15.1.2",
    "sequelize": "^6.37.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "nodemon": "^3.1.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  }
}
